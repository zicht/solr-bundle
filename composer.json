{
    "name": "zicht/solr-bundle",
    "type": "symfony-bundle",
    "description": "Solr utilities for Symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "doctrine/common": "^2.2 || ^3",
        "doctrine/orm": "^2.5",
        "guzzlehttp/guzzle": "^6.3",
        "sonata-project/admin-bundle": "^4",
        "symfony/console": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "zicht/framework-extra-bundle": "^10",
        "zicht/url-bundle": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\SolrBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zicht\\Bundle\\SolrBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analysis": "psalm",
        "test": "phpunit"
    }
}
