{
    "name": "zicht/solr-bundle",
    "type": "symfony-bundle",
    "description": "Solr utilities for Symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "doctrine/common": "^2.2 || ^3",
        "guzzlehttp/guzzle": "^6.3",
        "sonata-project/admin-bundle": "^3.74",
        "symfony/console": "^4.4",
        "symfony/framework-bundle": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\SolrBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zicht\\Bundle\\SolrBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
