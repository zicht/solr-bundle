{
    "name": "zicht/solr-bundle",
    "description": "Solr utilities for Symfony",
    "type": "symfony-bundle",
    "repositories": [],
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/symfony": ">=2.1",
        "psr/simple-cache": "^1",
        "psr/http-message": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4",
        "symfony/symfony": "^2.7"
    },
    "autoload": {
        "psr-0": {
            "": [
                "src/"
            ]
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "minimum-stability": "dev",
    "license": "MIT"
}
